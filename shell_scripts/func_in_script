#!~/Documents/shell_scripts/func_in_script
# an example of script functions

TITLE="System Information Report For $USER"

CURRENT_TIME=$(date +"%x %r %Z")
TIMESTAMP="Generated by $USER at $CURRENT_TIME"

func1 () {
	echo "func1 executed."
	return 0
}

func2 () {
	echo "func2 executed."
	return
}


cat << _EOF_
$TITLE 

$TIMESTAMP

$(func1)
$(func2)
_EOF_
