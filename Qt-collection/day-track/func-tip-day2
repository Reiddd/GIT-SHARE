QObject::installEventFilter(QWidget* parent)
/*
  overload 'eventFilter' function in parent:
  bool parent::eventFilter(QObject* obj, QWidget* parent);
*/


QLineEdit::setMaxLength(qint32 );
/*
  limit the number of characters you can enter in
*/


QLineEdit::setText(tr(QString));
/*
  QString can be displayed in the lineEdit to give user a hint
*/


QLineEdit::setEchoMode(echoMode);
/*
  echoMode:
  QLineEdit::NoEcho : no access to enter words in
  QLineEdit::Password : all characters will be replaced by '*'
*/


QSpinBox::setRange(qint32 begin, qint32 end);
QSpinBox::setPrefix(QString);
QSpinBox::setSuffix(QString);
/*
  basic setup for spinbox
*/


QFile* file = new QFile(QString);
QFile::open(QIODevice::ReadOnly | QIODevice::Text);


QFile::atEnd()
/*
  return true when meeting EOF
*/


QFileDialog::getOpenFileName(QWidget* parent, QString& windowName, QString& defaultPath, QString& typeFilter);
/*
  open a filedialog to let user chose file and path
*/
