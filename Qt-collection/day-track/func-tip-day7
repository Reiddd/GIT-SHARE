QWidget::setStyleSheet("*[mandatoryField=\"true\"]{background-color: yellow}");
QWidget::setProperty("mandatoryField",true);


QComboBox::addItem(tr("DashDotDotLine"),static_cast<int>(Qt::DashDotDotLine));


QComboBox::activated(int index);
QComboBox::activated(QString text);
/*
  QComboBox's SIGNAL, triggered when the value is chosen, even it's repeated
*/


QSpinBox::valueChanged();
QCheckBox::toggled();


QObject::connect(antialiasingCheckBox,SIGNAL(toggled(bool)),renderArea,SLOT(setAntialiased(bool)));


QPainterPath::closeSubpath();
/*
  automatically link the last point to the first point, and reset the current point to the (0,0)
*/
